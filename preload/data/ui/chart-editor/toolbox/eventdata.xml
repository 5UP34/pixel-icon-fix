<?xml version="1.0" encoding="utf-8"?>
<collapsible-dialog id="toolboxEvents" title="Events" width="300" height="200">
	<vbox width="100%" height="100%">
		<grid columns="2" width="100%">
			<label text="Event Type" verticalAlign="center" />
			<dropdown id="toolboxEventsEventKind" width="200" value="normal">
				<data />
			</dropdown>
		</grid>
		<frame id="toolboxEventsDataFrame" text="Event Data" width="100%">
			<grid id="toolboxEventsDataGrid" columns="2" width="100%" />
		</frame>
	</vbox>
</collapsible-dialog>
