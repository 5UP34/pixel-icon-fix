<?xml version="1.0" encoding="utf-8"?>

<collapsible-dialog id="toolboxDifficulty" title="Difficulty" width="300" height="400">
    <vbox>
        <frame text="Select Difficulty">
            <tree-view id="difficultyToolboxTree" width="260" height="150" />
        </frame>
        <hbox>
            <button allowFocus="false" disabled="true" id="difficultyToolboxAddVariation" text="Add Variation" />
            <button allowFocus="false" disabled="true" id="difficultyToolboxAddDifficulty" text="Add Difficulty" />
        </hbox>
        <hbox>
            <button allowFocus="false" id="difficultyToolboxSaveMetadata" text="Save Metadata" />
            <button allowFocus="false" id="difficultyToolboxSaveChart" text="Save Chart" />
            <button allowFocus="false" id="difficultyToolboxSaveAll" text="Save All" />
        </hbox>
        <hbox>
            <button allowFocus="false" id="difficultyToolboxLoadMetadata" text="Load Metadata" />
            <button allowFocus="false" id="difficultyToolboxLoadChart" text="Load Chart" />
        </hbox>
    </vbox>
</collapsible-dialog>