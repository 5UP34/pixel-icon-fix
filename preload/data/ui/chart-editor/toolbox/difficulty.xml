<?xml version="1.0" encoding="utf-8"?>
<collapsible-dialog id="toolboxDifficulty" title="Difficulty" width="300" height="425">
    <vbox>
        <frame text="Select Difficulty">
            <tree-view id="difficultyToolboxTree" width="260" height="200" />
        </frame>
        <hbox>
            <button allowFocus="false" id="difficultyToolboxAddVariation" text="Add Variation" />
            <button allowFocus="false" id="difficultyToolboxRemoveVariation" text="Remove Variation" />
        </hbox>
        <rule />
        <hbox>
            <button allowFocus="false" id="difficultyToolboxAddDifficulty" text="Add Difficulty" />
            <button allowFocus="false" id="difficultyToolboxRemoveDifficulty" text="Remove Difficulty" />
        </hbox>
        <rule />
        <hbox>
            <button allowFocus="false" id="difficultyToolboxSaveMetadata" text="Save Metadata" />
            <button allowFocus="false" id="difficultyToolboxSaveChart" text="Save Chart" />
            <button allowFocus="false" id="difficultyToolboxSaveAll" text="Save All" />
        </hbox>
        <hbox>
            <button allowFocus="false" id="difficultyToolboxLoadMetadata" text="Load Metadata" />
            <button allowFocus="false" id="difficultyToolboxLoadChart" text="Load Chart" />
        </hbox>
    </vbox>
</collapsible-dialog>
